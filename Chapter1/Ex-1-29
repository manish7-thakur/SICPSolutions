(define (simp f a b n)
    (define h (/ (- b a) n))
    (define (term k)
        (define y (f (+ a (* k h))))
            (if (or (= k 0) (= k n)) y
                (if(even? k) (* 2 y) (* 4 y))))
           (* (sum term 0 inc n) (/ h 3)))

(define (sum term a next b)
  (if (> a b)
      0
      (+ (term a)
         (sum term (next a) next b))))

(define (cube x) (* x x x))

(define (inc n) (+ n 1))